{
  "name": "sw-register-webpack-plugin",
  "version": "1.0.20",
  "description": "a webpack plugin for serviceWorker register and serviceWorker no-cache soultion",
  "main": "src/index.js",
  "scripts": {
    "build": "sh build.sh",
    "lint": "eslint ./{exmaples,src,test,templates}/**/*.{js,html}",
    "lint-fix": "yarn lint --fix",
    "test": "npm run lint && npm run build && ./node_modules/.bin/nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lavas-project/sw-register-webpack-plugin.git"
  },
  "keywords": [
    "no-cache",
    "plugin",
    "serviceWorker",
    "webpack",
    "sw-regiser"
  ],
  "author": "mj (zoumiaojiang@gmail.com)",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.25.0",
    "etpl": "^3.2.0",
    "minimatch": "^3.0.4"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "ava": {
    "concurrency": 5,
    "failFast": true,
    "files": [
      "test/*.test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "devDependencies": {
    "ava": "^1.0.1",
    "babel-cli": "^6.24.0",
    "babel-eslint": "^10.0.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.23.0",
    "bluebird": "^3.4.7",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "memory-fs": "^0.4.1",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.0",
    "webpack": "^4.28.1"
  }
}
